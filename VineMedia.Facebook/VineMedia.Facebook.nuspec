<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>FacebookAuth</id>
        <version>1.0.4</version>
        <title>Facebook Authentication Provider</title>
        <authors>gslaven</authors>
        <owners>gslaven</owners>
        <projectUrl>https://github.com/VineMedia/VineMedia.Facebook</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Library for Authenticating with Facebook OAuth and populating the Profile object with information from the user's open graph. It is designed to work with FormsAuthentication, it will create users and populate their profile when they log in &amp; authorise your application.</description>
        <tags>facebook authentication</tags>
        <dependencies>
            <dependency id="Facebook" version="6.0.22" />
            <dependency id="Castle.Core" version="3.0.0.4001" />
            <dependency id="Castle.Windsor" version="3.0.0.4001" />
        </dependencies>
    </metadata>
    <files>
        <file src="web.config.transform" target="content\web.config.transform" />
    </files>
</package>